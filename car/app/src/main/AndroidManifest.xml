<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.carKonlinCode">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.carKonlinCode.infer_change"
            android:screenOrientation="landscape"
            />

        <activity android:name="com.example.carKonlinCode.camera_switch" />
        <activity
            android:name="com.example.carKonlinCode.item_select"
            android:screenOrientation="landscape" />
        <activity android:name=".joystick" />
        <activity
            android:name="com.example.carKonlinCode.setting"
            android:label="@string/title_activity_setting"
            android:screenOrientation="landscape"
            android:theme="@style/setting_bar" />
        <activity
            android:name="com.example.carKonlinCode.start_tap"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.carKonlinCode.camera"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.carKonlinCode.check"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.carKonlinCode.MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>